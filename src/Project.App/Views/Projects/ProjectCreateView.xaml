<?xml version="1.0" encoding="utf-8" ?>
<views:ContentPageBase xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                       xmlns:m="clr-namespace:Project.BL.Models;assembly=Project.BL"
                       xmlns:vm="clr-namespace:Project.App.ViewModels"
                       xmlns:views="clr-namespace:Project.App.Views"
                       x:Class="Project.App.Views.Projects.ProjectCreateView"
                       x:DataType="vm:ProjectCreateViewModel"
                       Title="Projects">

    
    <VerticalStackLayout>
        <Label Text="In createView"
               TextColor="Red"></Label>
        <Label
            Text="Name">
        </Label>
        <Entry
            Text="{Binding Project.Name}">
        </Entry>

        <Button
            Text="Save"
            Command="{Binding SaveProjectCommand}">
        </Button>
    </VerticalStackLayout>
    
</views:ContentPageBase>
